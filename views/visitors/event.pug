doctype html
html
    title
    meta(content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1", name="viewport")
    head
        link(rel='icon', href='../img/icoH4.png', type='image/x-icon')
        link(rel='shortcut icon', href='../img/icoH4.png', type='image/x-icon')
        link(rel='stylesheet', media=screen, href='/css/visitor.css')
    body(cz-shortcut-listen="true")
        #page-region
            .main-region#main-region
                .date-time-step
                    .header
                        .js-profile-region
                            .wrapper
                                .icon-arrow-left.js-step-back.step-back
                                .title-wrapper
                                    div
                                        img(class='avatar js-avatar mbs clickable', src='/img/shppLogo.png')
                                .mbs.phs.popover-holder
                                    .increased.popover-toggle.silent.disabled
                    .subheader
                        .row.wrapper
                            .col1of2
                                .marker
                                .last-col
                            .col1of2.js-timezone-toggle-region.mobile-text-left.text-right
                                .right.mobile-full
                                    .popover-holder= 'Время:   '
                                        span.popover-toggle.disabled



        script(src="/js/lib/jquery-3.3.1.js")
        script(src='/js/date-time-handler.js')
        script(src='/js/lib/moment-with-locales.js')
        script.
            var holandlyUser = !{JSON.stringify(username)},
                holandlyData = !{JSON.stringify(userEvents)},
                holandlyPath = !{JSON.stringify(eventType)},
                holandlyEvents = !{JSON.stringify(typeEvents)},
                all = $.when(holandlyUser, holandlyData, holandlyPath, holandlyEvents);
            all.done(function () {
                window.holandlyUser = holandlyUser;
                window.holandlyPath = holandlyPath;
                window.holandlyData = holandlyData.userEvents;
                window.holandlyEvents = holandlyEvents;
            })